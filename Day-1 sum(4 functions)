#include<stdio.h>
int inp();
int cal();
int out();
int n1, n2, n3,sum;

int main()
{
  inp();
  cal();
  out();
  return 0;
}

int inp()
{
  printf("Enter 1st no:\n");
  scanf("%d", &n1);

  printf("Enter 2nd no:\n");
  scanf("%d",&n2);
  return 0;
}

int cal()
{
  sum = n1+n2;
  return 0;
}

int out()
{
  printf("The sum is: %d", sum);
  return 0;
}
